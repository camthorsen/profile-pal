{
  "manifest_version": 3,
  "name": "DevTools extension starter",
  "version": "0.1.0",
  "description": "Starter template for a Chrome DevTools extension.",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://skypilot.atlassian.net/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "devtools_page": "devtools-loader.html",
  "permissions": ["scripting"],
  "host_permissions": ["https://skypilot.atlassian.net/gateway/api/gasv3/api/v1/batch"]
}
